# ===========================================
# Realtime Service - Environment Variables
# ===========================================
# Copy this file to .env and update values

# Redis Configuration (Must match Relay Server)
REDIS_URL=your-redis-host:6379
REDIS_PASSWORD=your-redis-password
REDIS_DB=0

# Stream Configuration
STREAM_NAME=webhook-stream
DEAD_LETTER_QUEUE=webhook-dlq

# Service Configuration
PORT=3004

# Relay Client Port (for health checks)
CLIENT_PORT=3003

# CORS Origins (comma-separated)
CORS_ORIGINS=*
